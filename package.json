{
  "name": "quicktechpro-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm --workspace @quicktechpro/web run dev",
    "build:web": "npm --workspace @quicktechpro/web run build",
    "dev:api": "npm --workspace @quicktechpro/api run start:dev",
    "build:api": "npm --workspace @quicktechpro/api run build"
  },
  "dependencies": {
    "react-feather": "^2.0.10",
    "react-hot-toast": "^2.6.0"
  }
}
